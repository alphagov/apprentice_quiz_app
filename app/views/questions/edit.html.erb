<%= render "govuk_publishing_components/components/layout_for_admin", {
  environment: "production",
  product_name: "Publishing",
  browser_title: "Edit Question"
} do %>
  <%= render "govuk_publishing_components/components/layout_header", { environment: "production" } %>

  <div class="govuk-width-container">
    <%= render "govuk_publishing_components/components/back_link", href: quiz_path(@quiz) %>
    <main class="govuk-main-wrapper" id="main-content">
    </main>
  </div>

  <%= render "govuk_publishing_components/components/layout_footer", {} %>
<% end %>
